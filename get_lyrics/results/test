        Math.random() < (config.sra_rollout_percentage / 100);

      ads.initialize({config: config, targeting: targeting, initial_units: initial_units});

      if (!config.sra_enabled) {
        ads.ensure_targeting_for_placements([["desktop_song_leaderboard","desktop_song_leaderboard"],["desktop_song_sidebar_top","desktop_song_sidebar_top"],["desktop_song_medium1","desktop_song_medium1"]]);
      }
    });
  </script>

